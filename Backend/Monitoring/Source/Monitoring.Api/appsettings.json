{
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "FilePath": ".\\..\\..\\logs\\monitoring.api\\log_.log",
    "MaxFileSizeInMB": 100,
    "MaxRetainedFileCount": 14,
    "MinimumLevel": "Information",
    "RowTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} {Properties:j}{NewLine}{Exception}"
  }
}
